{
  "expo": {
    "name": "LevelUp",
    "slug": "levelUp",
    "version": "1.8.0",
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "scheme": "levelup",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash_fullscreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#121212"
    },
    "ios": {
      "bundleIdentifier": "com.arnav.levelup",
      "buildNumber": "15",
      "supportsTablet": false,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.arnav.levelup",
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#121212"
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "resize"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/splash.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8856df9f-fcea-4dac-a2e2-b61c0100ee03"
      }
    }
  }
}
