{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "babel.config.ts"  ],
  "exclude": [
    "node_modules",
    "**/__tests__/*",
    "**/__mocks__/*",
    "**/__rsc_tests__/*"
  ]
}
